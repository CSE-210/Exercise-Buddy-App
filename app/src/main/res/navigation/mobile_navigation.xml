<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_matches">

    <fragment
        android:id="@+id/nav_matches"
        android:name="com.example.activeamigo.ui.home.HomeFragment"
        android:label="@string/menu_matches"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_calender"
        android:name="com.example.activeamigo.ui.gallery.GalleryFragment"
        android:label="@string/menu_calender"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_preferences"
        android:name="com.example.activeamigo.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_preferences"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.activeamigo.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_settings"
        tools:layout="@layout/fragment_slideshow" />
